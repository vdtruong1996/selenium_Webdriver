<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net, Version=1.2.10.0, Culture=neutral, PublicKeyToken=1b44e1d426115821" />
	</configSections>
	<!-- Log4net Logging Setup -->
		<log4net debug="true">
			<appender name="FileAppender" type="log4net.Appender.FileAppender,log4net">
				<file value="D:\\auto\\log\\Logfile5.txt" />
				<!-- the location where the log file would be created -->
				<appendToFile value="true" />
				<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="----------------------------------------------------------
								   -----------------------------------------------------------------
								   %newline%date [%thread] %newline%date %level %newline%date %logger - 
								   %newline%date  %newline %message %newline  %newline%date %aspnet-context
								   %newline%date %type  %newline%date %aspnet-request  %newline%date %aspnet-session 
								   %newline%date %file  %newline %line %newline %property " />
				</layout>
				<filter type="log4net.Filter.LevelRangeFilter">
					<levelMin value="INFO" />
					<levelMax value="FATAL" />
				</filter>
			</appender>
			<root>
				<level value="DEBUG" />
				<!--<level value="Info" />
				<level value="Warn" />
				<level value="Error" />
				<level value="Fatal" />-->
				<!--<level value="All" />-->
				<appender-ref ref="FileAppender" />
			</root>
		</log4net>
</configuration>	
